{
    "name": "obadaaz/laravel-autocrud",
    "description": "Automatically generate CRUD operations for Laravel models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "obada",
            "email": "opadaalziede@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ObadaAz\\AutoCrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ObadaAz\\AutoCrud\\AutoCrudServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68",
        "pestphp/pest": "^3.7",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
